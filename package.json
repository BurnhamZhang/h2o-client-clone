{
  "name": "h2o-client",
  "version": "0.0.1",
  "description": "飞翔的送水员",
  "main": "index.js",
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "postinstall": "npm run build",
    "build": "babel-node config/build.js",
    "prod": "better-npm-run prod",
    "test": "better-npm-run test",
    "prod:dev": "better-npm-run prod:dev",
    "dev": "better-npm-run dev",
    "dev:prod": "better-npm-run dev:prod",
    "dev:mock": "better-npm-run dev:mock"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.cbpmgt.com:fe-team/h2o-client.git"
  },
  "betterScripts": {
    "prod": {
      "command": "nodemon --exec 'babel-node src/server/app.js'",
      "env": {
        "NODE_ENV": "production",
        "DATABASE": "prod"
      }
    },
    "test": {
      "command": "nodemon --exec 'babel-node src/server/app.js'",
      "env": {
        "NODE_ENV": "production",
        "DATABASE": "test"
      }
    },
    "prod:dev": {
      "command": "nodemon --exec 'babel-node src/server/app.js'",
      "env": {
        "NODE_ENV": "production",
        "DATABASE": "dev"
      }
    },
    "dev": {
      "command": "babel-node src/server/app.js",
      "env": {
        "NODE_ENV": "development",
        "DATABASE": "dev"
      }
    },
    "dev:prod": {
      "command": "babel-node src/server/app.js",
      "env": {
        "NODE_ENV": "development",
        "DATABASE": "prod"
      }
    },
    "dev:mock": {
      "command": "babel-node src/server/app.js",
      "env": {
        "NODE_ENV": "development",
        "DATABASE": "mock"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.3.2",
    "antd-mobile": "^0.9.9",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "better-npm-run": "0.0.13",
    "co-views": "^2.1.0",
    "css-loader": "^0.25.0",
    "debounce": "^1.0.0",
    "file-loader": "^0.9.0",
    "koa": "^1.2.4",
    "koa-mount": "^1.3.0",
    "koa-proxy": "^0.6.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "koa.io": "0.0.3",
    "moment": "^2.15.2",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "postcss-loader": "^1.0.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "swig": "^1.4.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.16.0",
    "babel-plugin-import": "^1.0.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chalk": "^1.1.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "koa-webpack-dev-middleware": "^1.2.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "ora": "^0.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.7.0",
    "webpack-dev-server": "^1.16.2"
  }
}
